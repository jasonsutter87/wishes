<h1>Welcome to the home page</h1>
<% if !current_user %>
	<p>Im sorry you must log in to see these wishes</p>
<% else %>
	<div class="container">
	  <div class="row">
	    <div class="col-sm-4">
	    	<% @wishes.each do |wish| %>
	    		<a href="/wishes/<%= wish.id%>"><%= wish.title%></a>
	    	<% end %>
	    </div>
	  </div>
	</div>
<% end %>