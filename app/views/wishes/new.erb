<div class="container">
  <div class="col-sm-10">
  <form action='/wishes' method='POST'>
   
   <fieldset class="form-group">
    <label for="title">Title</label>
    <input type="text" class="form-control" id="title"  name="wish[title]"placeholder="Place an amazing title here">
  </fieldset>

  <fieldset class="form-group">
    <label for="content">Content</label>
    <input type="textarea" class="form-control" id="content" name="wish[content]" placeholder="Cool awesome wish goes here">
  </fieldset>

  <button type="submit" class="btn btn-primary">Submit</button>
</form>
</div>